<HomeScreen>
    transition: NoTransition()
    name: "home"
    FloatLayout:
        Label: 
            text: "Clue Solver"
            font_size: '50sp'
            size_hint: (1, 0.5)
            pos_hint: {"x": 0.0, "y": 0.5}
        Button:
            text: "Start New Game"
            size_hint: (0.25, 0.25)
            pos_hint: {"x":0.125, "y":0.25}
            on_release: 
                root.clearOldData()
                app.root.current = "PlayerSelectScreen"
                root.manager.transition.direction = "left"
        Button:
            text: "Load Previous Game"      
            size_hint: (0.25, 0.25)
            pos_hint: {"x":0.625, "y":0.25}            
            on_release: 
                app.root.current = "load"
                root.manager.transition.direction = "left"


