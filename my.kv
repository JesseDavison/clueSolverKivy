<LoadGameScreen>
    name: "load"
    FloatLayout:
        Button:
            text: "Back"
            background_color: 0, 1, 0.2, 1
            size_hint: (0.0625, 0.0625)
            pos_hint: {"x":0.0625 / 2, "y":1 - 1.5 * 0.0625}
            on_release: 
                app.root.current = "home"
                root.manager.transition.direction = "right"
        Button:
            text: "Next"
            id: next_button
            background_color: 0, 1, 0.2, 1
            size_hint: (0.0625, 0.0625)
            pos_hint: {"x":1 - 1.5 * 0.0625, "y":1 - 1.5 * 0.0625}
            disabled: True
            on_press: 
                root.confirmLoadGame()
                app.root.current = "ExecuteTurnScreen"
                # root.manager.transition.direction = "left"     
        Spinner:
            id: fileChooserSpinner
            text: 'Click here to choose a file to load:'
            values: []
            on_text: root.spinnerSelect()
            size_hint: (0.8, 0.1)
            pos_hint: {"x": 0.1, "y": 0.7}
        Button:
            id: delete_file_button
            text: "DELETE SELECTED FILE"
            disabled: True
            background_color: 1, 0, 0, 1
            size_hint: (0.3, 0.1)
            pos_hint: {"x": 0.6, "y": 0.05}
            on_press:
                root.deleteSelectedFile()



