<ExecuteTurnScreen>
    name: "ExecuteTurnScreen"
    FloatLayout:
        # SECTION HEADER                                HEADERHEADERHEADERHEADERHEADERHEADERHEADERHEADERHEADERHEADERHEADERHEADERHEADERHEADERHEADERHEADERHEADERHEADER
        Label:
            id: title_label
            section: "header"
            # vrs: {'turnNumber': -1, 'player': '', 'type': ''}           
            defaultText: "Turn X, (player) suggests:" 
            text: "Turn X, (player) suggests:"
            text_color: 1, 1, 0, 1
            canvas.before:
                Color:
                    rgba: 0.3, 0.3, 0.3, 1
                Rectangle:
                    pos: self.pos
                    size: self.size
            size_hint: (0.6, 0.1)   
            pos_hint: {"x": 0.2, "y": 0.9}
        Label:
            id: filename_label
            text: 'filename placeholder text'
            text_size: self.size            # https://stackoverflow.com/questions/31638788/python-kivy-align-text-to-the-left-side-of-a-label
            halign: 'left'
            size_hint: (1, 0.05)
            pos_hint: {"x": 0.03, "y": 0.03}
            color: 0.3, 0.3, 0.3, 1
            # canvas.before:
            #     Color:
            #         rgba: 0.3, 0.3, 0.3, 1
            #     Rectangle:
            #         pos: self.pos
            #         size: self.size            
        Button:
            id: notes_button
            section: "header"
            type: "notesButton"
            defaultText: "Notes"
            text: "Analysis Table"
            background_color: 0, 1, 0.2, 1
            # font_size: '10sp'
            size_hint: (0.2, 0.075)
            pos_hint: {"x":0, "y":1 - 0.075}
            on_release: 
                # ANALYZE THE DATA BABY!
                root.analyzeTheData()
                #       goes to ANALYSIS TABLE
                app.root.current = "AnalysisTableScreen"
                # root.manager.transition.direction = "right"
        Button:
            id: past_turns_button
            section: "header"
            type: "pastTurnsButton"
            defaultText: "Past turns"
            text: "Past turns"
            # id: next_button
            # vrs: {'turnNumber': -1, 'player': '', 'type': 'next_button'}            
            background_color: 0, 1, 0.2, 1
            size_hint: (0.2, 0.075)
            pos_hint: {"x": 0, "y":1 - 2 * 0.075}
            disabled: False
            on_press: 
                root.printTheTurnsPretty()
                app.root.current = "SuggestionHistoryScreen"
                # root.manager.transition.direction = "left"  
        Button:
            id: homescreen_button
            # section: "header"
            # type: "notesButton"
            # defaultText: "Home Screen"
            text: "Home Screen"
            background_color: 0, 1, 0.2, 1
            # font_size: '10sp'
            size_hint: (0.2, 0.075)
            pos_hint: {"x": 0.8, "y":1 - 0.075}
            on_release: 
                app.root.current = "home"
                root.resetTurnHistoryScreen()
                # root.manager.transition.direction = "right"

        # SECTION ABLETOGUESS                    ABLETOGUESSABLETOGUESSABLETOGUESSABLETOGUESSABLETOGUESSABLETOGUESSABLETOGUESSABLETOGUESSABLETOGUESSABLETOGUESSABLETOGUESS
        Spinner:
            id: able_to_guess
            type: 'ableToGuess'
            section: "ableToGuess"
            defaultText: 'Able to suggest'
            # halign: 'left'
            text: 'Able to suggest'
            values: ['Able to suggest', 'NOT able']
            on_text: root.spinnerClicked(self)
            size_hint: (0.2, 0.08)
            pos_hint: {"x": 0.05, "y": 0.7}

        # SECTION A                             AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA
        Spinner:
            id: killer_spinner
            type: "killer"
            section: 'A'
            option_cls: 'DropDownCustomClass'
            defaultText: 'suggested killer'
            text: "suggested killer"
            values: ['0. Green', '1. Mustard', '2. Peacock', '3. Plum', '4. Scarlett', '5. Orchid']
            on_text: root.spinnerClicked(self)
            size_hint: (0.2, 0.08)
            pos_hint: {"x": 0.05 + 0.2 + 0.1 / 3, "y": 0.7}
        Spinner:
            id: weapon_spinner
            type: "weapon"
            section: 'A'
            option_cls: 'DropDownCustomClass'
            defaultText: 'suggested weapon'
            text: "suggested weapon"
            values: ['6. Candlestick', '7. Dagger', '8. Pipe', '9. Revolver', '10. Rope', '11. Wrench']
            on_text: root.spinnerClicked(self)
            size_hint: (0.2, 0.08)
            pos_hint: {"x": 0.05 + 0.2 * 2 + 2 * (0.1 / 3), "y": 0.7}
        Spinner:
            id: room_spinner
            type: "room"
            section: 'A'
